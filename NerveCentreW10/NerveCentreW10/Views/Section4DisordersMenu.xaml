<Page
    x:Class="NerveCentreW10.Views.Section4DisordersMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:NerveCentreW10.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    CacheMode="BitmapCache"
    NavigationCacheMode="Enabled"
    mc:Ignorable="d">

    <GridView
        x:Name="GridView1"
        ItemClick="GridView1_ItemClick"
        ItemContainerStyle="{StaticResource GridViewItemStyle1}"
        ItemTemplate="{StaticResource SectionDataTemplate}"
        ItemsSource="{x:Bind ViewModel.SubsectionList}"
        Style="{StaticResource GridViewStyle1}" />

</Page>
